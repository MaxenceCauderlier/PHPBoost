<section id="module-user-messages">
	<header class="section-header">
		<h1>{L_MESSAGES}</h1>
	</header>
	<div class="sub-section">
		<article class="messages-item several-items">
			<div class="content">
				<div class="available-modules-msg">
					# START available_modules_msg #
						<p>
							<a href="{available_modules_msg.U_LINK_USER_MSG}">
								# IF available_modules_msg.C_IMG_USER_MSG #<i class="{available_modules_msg.IMG_USER_MSG}" aria-hidden="true"></i># ENDIF #
								{available_modules_msg.NAME_USER_MSG}
								({available_modules_msg.MESSAGES_NUMBER})
							</a>
						</p>
					# END available_modules_msg #
				</div>
			</div>
		</article>
	</div>
	<footer></footer>
</section>
